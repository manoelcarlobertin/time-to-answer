<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Detalhes do Administrador</h1>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>#</th>
        <th>Email</th>
        <th>Criado em</th>
        <th>Atualizado em</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @admin.id %></td>
        <td><%= @admin.email %></td>
        <td><%= l(@admin.created_at, format: :long) %></td>
        <td><%= l(@admin.updated_at, format: :long) %></td>
      </tr>
    </tbody>
  </table>
</div>

<div class="row mt-3">
  <div class="col-lg-12">
    <%= link_to admins_backoffice_admins_path, class: "btn btn-primary" do %>
      <i class="fa fa-arrow-left"></i> Voltar
    <% end %>
    
    <%= link_to edit_admins_backoffice_admin_path(@admin), class: "btn btn-warning" do %>
      <i class="fa fa-edit"></i> Editar
    <% end %>
  </div>
</div>
